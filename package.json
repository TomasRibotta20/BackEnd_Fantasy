{
  "name": "BackEnd_Fantasy",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "db:update": "pnpm run build && node dist/scripts/updateSchemaSafe.js",
    "start:dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./dist/app.js\"",
    "seed": "pnpm run build && mikro-orm seeder:run --class=CreatePremiosSeeder",
    "lint": "eslint .",
    "format": "prettier --write .",
    "docs:build": "typedoc --out docs src --entryPointStrategy expand",
    "docs:watch": "typedoc --watch --out docs src --entryPointStrategy expand",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "pnpm test tests/unit",
    "test:integration": "pnpm test tests/integration",
    "test:watch": "pnpm test -- --watch",
    "test:coverage:unit": "node --experimental-vm-modules --max-old-space-size=4096 node_modules/jest/bin/jest.js --coverage tests/unit",
    "test:coverage:integration": "node --experimental-vm-modules --max-old-space-size=4096 node_modules/jest/bin/jest.js --coverage tests/integration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@mikro-orm/better-sqlite": "^6.4.16",
    "@mikro-orm/cli": "6.4.16",
    "@mikro-orm/seeder": "6.4.16",
    "@mikro-orm/sqlite": "6.4.16",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.0.12",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "better-sqlite3": "11.10.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "sql.js": "1.12.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsc-watch": "6.0.4",
    "typedoc": "^0.28.7",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@eslint/js": "^9.30.1",
    "@mikro-orm/core": "6.4.16",
    "@mikro-orm/mysql": "6.4.16",
    "@mikro-orm/sql-highlighter": "1.0.1",
    "BackEnd_Fantasy": "link:",
    "bcrypt": "^6.0.0",
    "cookie-parser": "1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "5.1.0",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^8.0.1",
    "groq-sdk": "^0.37.0",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "^7.0.6",
    "reflect-metadata": "0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.17"
  },
  "mikro-orm": {
    "useTsNode": false,
    "configPaths": [
      "./dist/shared/db/orm.js",
      "./src/shared/db/orm.ts"
    ]
  }
